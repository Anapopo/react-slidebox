{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "semi": 0,
        "curly": 1,
        "indent": 1,
        "eqeqeq": 1,
        "quotes": [1, "single"],
        "no-var": 1,
        "no-eval": 1,
        "no-trailing-spaces": 1,
        "no-useless-computed-key": 1,
        "dot-location": 1,
        "dot-notation": 1,
        "prefer-const": 1,
        "arrow-spacing": 1,
        "space-infix-ops": 1,
        "space-unary-ops": 1,
        "space-in-parens": 1,
        "space-before-function-paren": 1,
        "block-scoped-var": 1,
        "comma-spacing": 1,
        "comma-style": 1,
        "keyword-spacing": 1,
        "linebreak-style": 1,
        "lines-around-comment": 1,
        "spaced-comment": 1,
        "rest-spread-spacing": 1,
        "no-mixed-spaces-and-tabs": 1,
        "no-multiple-empty-lines": 1,
        "prefer-destructuring": 1
    }
}
